{
  "name": "exercise-angular-cli",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "ng serve --open --configuration production",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch --reporters spec",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer ./dist/customer-admin-app/stats.json",
    "format:write": "prettier \"projects/**/*.{ts,scss,json,html,js}\" --write",
    "format:test": "prettier \"projects/**/*.{ts,scss,json,html,js}\" --list-different"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.11",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.11",
    "@angular/compiler": "~11.2.11",
    "@angular/core": "~11.2.11",
    "@angular/forms": "~11.2.11",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.11",
    "@angular/platform-browser-dynamic": "~11.2.11",
    "@angular/router": "~11.2.11",
    "D": "^1.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.18",
    "@angular-eslint/builder": "4.3.1",
